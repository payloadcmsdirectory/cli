{
  "name": "@payloadcmsdirectory/cli",
  "version": "0.2.6",
  "description": "CLI tool for PayloadCMS plugins",
  "private": false,
  "bin": "./bin/index.js",
  "scripts": {
    "build": "rimraf dist && swc src -d dist --copy-files && tsc --emitDeclarationOnly",
    "dev": "swc src -d dist --copy-files -w",
    "lint": "eslint \"src/**/*.ts\"",
    "start": "node ./bin/index.js",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "bin"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "execa": "^5.1.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.104",
    "@types/inquirer": "^8.2.10",
    "@types/node": "^20.10.6",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
